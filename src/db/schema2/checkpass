const mongoose = require('mongoose')
const Mymodel = require('./schema2')

async function checkpass(PASS) 
{
    const Instance = await Mymodel.findOne()  
    
    if(Instance)
    {
        console.log(Instance.PASS)
        if(Instance.PASS == PASS)
            return true
        else
        return false
    }
    else 
    return false
}

module.exports = {checkpass}
